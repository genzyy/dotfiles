{
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "editor.fontFamily": "'Iosevka Nerd Font', 'CaskaydiaCove Nerd Font Mono', 'PragmataPro Mono Liga', monospace",
    "editor.fontSize": 15,
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "smooth",
    "window.restoreFullscreen": true,
    "window.zoomLevel": 0.3,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.minimap.enabled": false,
    "workbench.productIconTheme": "icons-carbon",
    // Font Settings
    "editor.fontWeight": "500",
    "editor.fontLigatures": true,
    "editor.tabCompletion": "on",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "breadcrumbs.enabled": true,
    "explorer.openEditors.visible": 0,
    // Bracket Pair Colorizer and Prettier Settings
    "bracket-pair-colorizer-2.colors": [
        "#F72585",
        "#94b4a4",
        "#a3d8f4"
    ],
    "prettier.singleQuote": true,
    "prettier.jsxSingleQuote": true,
    "prettier.trailingComma": "none",
    "prettier.arrowParens": "avoid",
    "prettier.proseWrap": "preserve",
    "prettier.quoteProps": "as-needed",
    "prettier.jsxBracketSameLine": false,
    "prettier.bracketSpacing": true,
    "prettier.tabWidth": 2,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.sideBar.location": "right",
    // Import Cost Extension Settings
    // Upper size limit, in KB, that will count a package as a small package
    "importCost.smallPackageSize": 50,
    // Upper size limit, in KB, that will count a package as a medium package
    "importCost.mediumPackageSize": 100,
    // Decoration color for small packages
    "importCost.smallPackageColor": "#7cc36e",
    // Decoration color for medium packages
    "importCost.mediumPackageColor": "#7cc36e",
    // Decoration color for large packages
    "importCost.largePackageColor": "#d44e40",
    // File extensions to be parsed by the Typescript parser
    "importCost.typescriptExtensions": [
        "\\.tsx?$"
    ],
    // File extensions to be parsed by the Javascript parser
    "importCost.javascriptExtensions": [
        "\\.jsx?$"
    ],
    // Which bundle size to display
    "importCost.bundleSizeDecoration": "both",
    // Display the 'calculating' decoration
    "importCost.showCalculatingDecoration": true,
    // Print debug messages in output channel
    "importCost.debug": false,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "newUntitledFile",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorCustomizations": {
        "[Forest Night]": {
            "activityBarBadge.background": "#1E1C31",
            "activityBar.background": "#100E23",
            "editor.background": "#1E1C31",
            "editor.foreground": "#CBE3E7",
            "editor.lineHighlightBackground": "#100E23",
            "editor.selectionBackground": "#585273",
            "editorCursor.foreground": "#91DDFF",
            "editorIndentGuide.activeBackground": "#585273",
            "editorWhitespace.foreground": "#3E3859",
            "editor.findMatchBackground": "#585273",
            "editor.findMatchHighlightBackground": "#3E3859",
            "editorWidget.background": "#2D2B40",
            "sideBar.background": "#100E23",
            "sideBar.foreground": "#CBE3E7",
            "input.background": "#3E3859",
            "input.foreground": "#CBE3E7",
            "input.border": "#585273",
            "inputValidation.errorBorder": "#F02E6E",
            "inputValidation.errorBackground": "#F02E6E",
            "inputValidation.errorForeground": "#F48FB1",
            "dropdown.background": "#3E3859",
            "dropdown.foreground": "#CBE3E7",
            "dropdown.border": "#585273",
            "dropdown.listBackground": "#3E3859",
            "button.background": "#585273",
            "button.foreground": "#CBE3E7",
            "focusBorder": "#A37ACC",
            "statusBar.background": "#100E23",
            "tab.activeBackground": "#1E1C31",
            "tab.inactiveBackground": "#2D2B40",
            "editorGroupHeader.tabsBackground": "#2D2B40",
            "editorGroupHeader.noTabsBackground": "#2D2B40",
            "tab.border": "#2D2B40",
            "selection.background": "#91DDFF",
            "list.hoverBackground": "#2D2B40",
            "list.inactiveSelectionBackground": "#3E3859",
            "list.activeSelectionBackground": "#585273"
        },
        "tokenColors": [
            {
                "name": "Comment",
                "scope": "comment",
                "settings": {
                    "foreground": "#8A889D"
                }
            },
            {
                "name": "String",
                "scope": "string",
                "settings": {
                    "foreground": "#FFE6B3"
                }
            },
            {
                "name": "Constants",
                "scope": [
                    "constant.numeric",
                    "constant.language",
                    "constant.character",
                    "constant.other"
                ],
                "settings": {
                    "foreground": "#F2B482"
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "name": "Keyword, Storage",
                "scope": [
                    "keyword",
                    "storage"
                ],
                "settings": {
                    "foreground": "#91DDFF"
                }
            },
            {
                "scope": "storage",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#A1EFD3",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "Class",
                "scope": "entity.name.class",
                "settings": {
                    "foreground": "#F48FB1",
                    "fontStyle": "underline"
                }
            },
            {
                "scope": "entity.other.inherited-class",
                "settings": {
                    "foreground": "#F48FB1",
                    "fontStyle": "italic underline"
                }
            },
            {
                "name": "Function",
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#F48FB1",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#CBE3E7",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "entity.name.tag",
                "settings": {
                    "foreground": "#F48FB1",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.other.attribute-name",
                "settings": {
                    "foreground": "#A1EFD3",
                    "fontStyle": ""
                }
            },
            {
                "scope": "support.function",
                "settings": {
                    "foreground": "#63F2F1",
                    "fontStyle": ""
                }
            },
            {
                "scope": "support.constant",
                "settings": {
                    "foreground": "#63F2F1",
                    "fontStyle": ""
                }
            },
            {
                "scope": "support.type, support.class",
                "settings": {
                    "foreground": "#63F2F1",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "support.other.variable",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "invalid",
                "settings": {
                    "foreground": "#CBE3E7",
                    "background": "#F02E6E",
                    "fontStyle": ""
                }
            },
            {
                "scope": "invalid.deprecated",
                "settings": {
                    "foreground": "#CBE3E7",
                    "background": "#A37ACC"
                }
            },
            {
                "scope": "meta.structure.dictionary.json string.quoted.double.json",
                "settings": {
                    "foreground": "#FFE6B3"
                }
            },
            {
                "scope": "meta.diff, meta.diff.header",
                "settings": {
                    "foreground": "#F2B482"
                }
            },
            {
                "scope": "markup.deleted",
                "settings": {
                    "foreground": "#F48FB1"
                }
            },
            {
                "scope": "markup.inserted",
                "settings": {
                    "foreground": "#62D196"
                }
            },
            {
                "scope": "markup.changed",
                "settings": {
                    "foreground": "#F2B482"
                }
            },
            {
                "scope": "markup.deleted.git_gutter",
                "settings": {
                    "foreground": "#F02E6E"
                }
            },
            {
                "scope": "markup.inserted.git_gutter",
                "settings": {
                    "foreground": "#62D196"
                }
            },
            {
                "scope": "markup.changed.git_gutter",
                "settings": {
                    "foreground": "#F2B482"
                }
            },
            {
                "scope": "markup.ignored.git_gutter",
                "settings": {
                    "foreground": "#8A889D"
                }
            },
            {
                "scope": "markup.untracked.git_gutter",
                "settings": {
                    "foreground": "#8A889D"
                }
            },
            {
                "scope": "constant.numeric.line-number.find-in-files - match",
                "settings": {
                    "foreground": "#A37ACC"
                }
            },
            {
                "scope": "entity.name.filename.find-in-files",
                "settings": {
                    "foreground": "#FFE6B3"
                }
            },
            {
                "scope": "token.info-token",
                "settings": {
                    "foreground": "#65B2FF"
                }
            },
            {
                "scope": "token.warn-token",
                "settings": {
                    "foreground": "#F2B482"
                }
            },
            {
                "scope": "token.error-token",
                "settings": {
                    "foreground": "#F02E6E"
                }
            },
            {
                "scope": "token.debug-token",
                "settings": {
                    "foreground": "#A37ACC"
                }
            }
        ]
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "Mayukai Semantic Mirage",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}